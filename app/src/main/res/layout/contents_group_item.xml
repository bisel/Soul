<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data class="MeditationContentsBinding">

        <variable
            name="viewModel"
            type="com.soulfriends.meditation.viewmodel.MeditationContentsViewModel">

        </variable>
<!--        <import type="com.soulfriends.meditation.viewmodel.MeditationContentsViewModel.MeditationContents"/>-->

<!--     -->
    </data>

    <LinearLayout
        android:gravity="center"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="ExtraText">

        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@={viewModel.listMeditationContents.thumbnail}"
            >

        </TextView>

        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@={viewModel.listMeditationContents.title}"
            >

        </TextView>

    </LinearLayout>

</layout>